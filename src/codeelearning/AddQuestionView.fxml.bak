    <Pane id="imagePane" layoutX="15.0" layoutY="16.0" prefHeight="181.0" prefWidth="353.0">
      <children>
        <ImageView id="QuestionFigure" fx:id="questionFigure" layoutX="15.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
          <fitHeight>
            <Long fx:value="150" />
          </fitHeight>
          <fitWidth>
            <Long fx:value="300" />
          </fitWidth>
        </ImageView>
        <Label id="label" fx:id="labelFile" layoutX="14.0" layoutY="148.0" minHeight="16.0" minWidth="69.0" prefHeight="16.0" prefWidth="69.0" />
      </children>
    </Pane>
    <Button id="button" fx:id="setFigureButton" layoutX="293.0" layoutY="120.0" onAction="#handleButtonAction" text="SetFigure" />
  </children>
</AnchorPane>
